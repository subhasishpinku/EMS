<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:id="@+id/screenTouch"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <FrameLayout
            android:id="@+id/fram"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints">
            <ImageView
                android:id="@+id/header_cover_image"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:scaleType="centerCrop"/>
            <ImageView
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_gravity="center"
                android:layout_marginTop="0dp"
                android:background="@mipmap/collagefromlogo"/>
        </FrameLayout>

        <ImageView
            android:layout_marginRight="20dp"
            android:id="@+id/Img_Id"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="20dp"
            android:background="@mipmap/logo_bar"/>

        <ScrollView
            android:layout_below="@+id/fram"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="-127dp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:id="@+id/sendInfoLv1"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"
                    android:background="@drawable/collage_from"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="140dp">
                    <ImageView
                        android:id="@+id/circule_image"
                        android:layout_marginTop="20dp"
                        android:layout_marginLeft="10dp"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:background="@drawable/circule_image"/>
                    <TextView
                        android:id="@+id/collage_id"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/circule_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="2dp"
                        android:layout_marginTop="25dp"
                        android:textStyle="bold"
                        android:text="KRISHNA NURSING INSTITUTE"
                        android:textColor="@color/white"
                        android:textSize="13sp" />
                    <TextView
                        android:id="@+id/phone_id"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/circule_image"
                        android:layout_below="@+id/collage_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="2dp"
                        android:text="Phone : 6290532924"
                        android:textColor="@color/white"
                        android:textSize="10sp" />
                    <TextView
                        android:id="@+id/web_id"
                        android:layout_alignParentEnd="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/phone_id"
                        android:layout_below="@+id/collage_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="2dp"
                        android:text="Website : kninstitute.in"
                        android:textColor="@color/white"
                        android:textSize="10sp" />
                    <TextView
                        android:id="@+id/led_tv5"
                        android:padding="10dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginRight="15dp"
                        android:layout_alignParentEnd="true"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:textColor="@color/white"
                        android:layout_marginLeft="0dp"
                        android:layout_below="@+id/web_id"
                        android:background="@drawable/application_froms"
                        android:layout_width="wrap_content"
                        android:layout_height="35dp"
                        android:text="College Application Form"/>
                </RelativeLayout>
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/SfName"
                    android:layout_below="@+id/sendInfoLv1"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="20sp"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:background="@drawable/edittextbg"
                    android:inputType="text"
                    android:textColor="#7c7c7c"
                    android:hint="Student First Name"
                    android:layout_marginRight="20dp"
                    android:textSize="15dp"
                    android:enabled="false"
                    android:paddingLeft="10dp"
                    android:paddingEnd="10dp" />
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/SmName"
                    android:layout_below="@+id/SfName"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10sp"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:background="@drawable/edittextbg"
                    android:enabled="false"
                    android:inputType="text"
                    android:textColor="#7c7c7c"
                    android:hint="Student Middle Name"
                    android:layout_marginRight="20dp"
                    android:textSize="15dp"
                    android:paddingLeft="10dp"
                    android:paddingEnd="10dp" />
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/SlName"
                    android:layout_below="@+id/SmName"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10sp"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:enabled="false"
                    android:background="@drawable/edittextbg"
                    android:inputType="text"
                    android:textColor="#7c7c7c"
                    android:hint="Student Last Name"
                    android:layout_marginRight="20dp"
                    android:textSize="15dp"
                    android:paddingLeft="10dp"
                    android:paddingEnd="10dp" />
                   <LinearLayout
                       android:id="@+id/lv"
                       android:layout_below="@+id/SlName"
                       android:weightSum="2"
                       android:orientation="horizontal"
                       android:layout_marginTop="5sp"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content">
                       <com.google.android.material.textfield.TextInputEditText
                           android:id="@+id/SfatherName"
                           android:layout_below="@+id/SmName"
                           android:layout_weight="1"
                           android:layout_marginLeft="20dp"
                           android:layout_marginTop="10sp"
                           android:layout_width="match_parent"
                           android:layout_height="50dp"
                           android:background="@drawable/edittextbg"
                           android:inputType="text"
                           android:textColor="#7c7c7c"
                           android:hint="Fatherâ€™s Name"
                           android:textSize="15dp"
                           android:paddingLeft="10dp"
                           android:paddingEnd="10dp" />

                       <com.google.android.material.textfield.TextInputEditText
                           android:id="@+id/SmotherName"
                           android:layout_below="@+id/SmName"
                           android:layout_weight="1"
                           android:layout_marginLeft="10dp"
                           android:layout_marginRight="20dp"
                           android:layout_marginTop="10sp"
                           android:layout_width="match_parent"
                           android:layout_height="50dp"
                           android:background="@drawable/edittextbg"
                           android:inputType="text"
                           android:textColor="#7c7c7c"
                           android:hint="Motherâ€™s Name"
                           android:textSize="15dp"
                           android:paddingLeft="10dp"
                           android:paddingEnd="10dp" />
                   </LinearLayout>
                <LinearLayout
                    android:id="@+id/lv1"
                    android:layout_below="@+id/lv"
                    android:weightSum="2"
                    android:orientation="horizontal"
                    android:layout_marginTop="5sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/mobileNoId"
                        android:layout_below="@+id/SmName"
                        android:layout_weight="1"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/edittextbg"
                        android:inputType="number"
                        android:enabled="false"
                        android:textColor="#7c7c7c"
                        android:hint="Mobile Number"
                        android:textSize="15dp"
                        android:paddingLeft="10dp"
                        android:paddingEnd="10dp" />
                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp">
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/dobName"
                                android:layout_width="135dp"
                                android:layout_height="match_parent"
                                android:background="@drawable/datecolors"
                                android:inputType="date"
                                android:textColor="#7c7c7c"
                                android:hint="Date of Birth"
                                android:textSize="15dp"
                                android:paddingLeft="10dp"
                                android:paddingEnd="10dp" />
                            <LinearLayout
                                android:layout_marginRight="20dp"
                                android:background="@drawable/bgright"
                                android:layout_width="40dp"
                                android:layout_height="50dp">
                                <ImageView
                                    android:background="@drawable/calenderss"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="5dp"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/paddress"
                    android:layout_below="@+id/lv1"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10sp"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:background="@drawable/edittextbg"
                    android:inputType="text"
                    android:textColor="#7c7c7c"
                    android:hint="Present Address"
                    android:layout_marginRight="20dp"
                    android:textSize="15dp"
                    android:paddingLeft="10dp"
                    android:paddingEnd="10dp" />
                <LinearLayout
                    android:id="@+id/lv2"
                    android:layout_below="@+id/paddress"
                    android:weightSum="2"
                    android:orientation="horizontal"
                    android:layout_marginTop="5sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/stateId"
                        android:layout_below="@+id/SmName"
                        android:layout_weight="1"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/edittextbg"
                        android:inputType="text"
                        android:textColor="#7c7c7c"
                        android:hint="State"
                        android:textSize="15dp"
                        android:paddingLeft="10dp"
                        android:paddingEnd="10dp" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/sDistrict"
                        android:layout_below="@+id/SmName"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/edittextbg"
                        android:inputType="text"
                        android:textColor="#7c7c7c"
                        android:hint="District"
                        android:textSize="15dp"
                        android:paddingLeft="10dp"
                        android:paddingEnd="10dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/lv3"
                    android:layout_below="@+id/lv2"
                    android:weightSum="2"
                    android:orientation="horizontal"
                    android:layout_marginTop="5sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/cityId"
                        android:layout_below="@+id/SmName"
                        android:layout_weight="1"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/edittextbg"
                        android:inputType="text"
                        android:textColor="#7c7c7c"
                        android:hint="City"
                        android:textSize="15dp"
                        android:paddingLeft="10dp"
                        android:paddingEnd="10dp" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/pinID"
                        android:layout_below="@+id/SmName"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/edittextbg"
                        android:inputType="text"
                        android:textColor="#7c7c7c"
                        android:hint="Pin"
                        android:textSize="15dp"
                        android:paddingLeft="10dp"
                        android:paddingEnd="10dp" />
                </LinearLayout>


                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/paddress1"
                    android:layout_below="@+id/lv3"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10sp"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:background="@drawable/edittextbg"
                    android:enabled="true"
                    android:inputType="text"
                    android:textColor="#7c7c7c"
                    android:hint="Permanent Address"
                    android:layout_marginRight="20dp"
                    android:textSize="15dp"
                    android:paddingLeft="10dp"
                    android:paddingEnd="10dp" />
                <LinearLayout
                    android:id="@+id/lv4"
                    android:layout_below="@+id/paddress1"
                    android:weightSum="2"
                    android:orientation="horizontal"
                    android:layout_marginTop="5sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/stateId1"
                        android:layout_below="@+id/SmName"
                        android:layout_weight="1"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/edittextbg"
                        android:inputType="text"
                        android:textColor="#7c7c7c"
                        android:hint="State"
                        android:textSize="15dp"
                        android:paddingLeft="10dp"
                        android:paddingEnd="10dp" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/sDistrict1"
                        android:layout_below="@+id/SmName"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/edittextbg"
                        android:inputType="text"
                        android:textColor="#7c7c7c"
                        android:hint="District"
                        android:textSize="15dp"
                        android:paddingLeft="10dp"
                        android:paddingEnd="10dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/lv5"
                    android:layout_below="@+id/lv4"
                    android:weightSum="2"
                    android:orientation="horizontal"
                    android:layout_marginTop="5sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/cityId1"
                        android:layout_below="@+id/SmName"
                        android:layout_weight="1"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/edittextbg"
                        android:inputType="text"
                        android:textColor="#7c7c7c"
                        android:hint="City"
                        android:textSize="15dp"
                        android:paddingLeft="10dp"
                        android:paddingEnd="10dp" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/pinID1"
                        android:layout_below="@+id/SmName"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/edittextbg"
                        android:inputType="text"
                        android:textColor="#7c7c7c"
                        android:hint="Pin"
                        android:textSize="15dp"
                        android:paddingLeft="10dp"
                        android:paddingEnd="10dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/lv6"
                    android:layout_below="@+id/lv5"
                    android:weightSum="2"
                    android:orientation="horizontal"
                    android:layout_marginTop="5sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/adherId"
                        android:layout_below="@+id/SmName"
                        android:layout_weight="1"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/edittextbg"
                        android:inputType="text"
                        android:textColor="#7c7c7c"
                        android:hint="Adhar Number"
                        android:textSize="15dp"
                        android:paddingLeft="10dp"
                        android:paddingEnd="10dp" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/genderId"
                        android:layout_below="@+id/SmName"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/edittextbg"
                        android:inputType="text"
                        android:textColor="#7c7c7c"
                        android:hint="Gender"
                        android:textSize="15dp"
                        android:paddingLeft="10dp"
                        android:paddingEnd="10dp" />
                </LinearLayout>

                <TextView
                    android:id="@+id/submitId"
                    android:padding="5dp"
                    android:layout_marginBottom="20sp"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="20dp"
                    android:textStyle="bold"
                    android:layout_centerInParent="true"
                    android:gravity="center"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:layout_marginLeft="20dp"
                    android:layout_below="@+id/lv6"
                    android:background="@drawable/application_froms"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:text="SUBMIT"/>
            </RelativeLayout>

        </ScrollView>
</RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>